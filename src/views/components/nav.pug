+b.fixed._md-top
	+b.nav._style#navigation
		+e.container.container
			+e.row.flex.flex-y-center
				+e.logo
					+hamburger('2')
					+e.logo-content
						+e.P.logo-content-text Kurochka
				+e.NAV.menu#js-navigation-menu
					for item in navigation
						+e.LI.menu-item
							+e.A.menu-item-link(data-scroll href=item.link)=item.name